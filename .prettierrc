{
  "semi": false,
  "singleQuote": false,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.ts", "*.js"],
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.json"],
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
